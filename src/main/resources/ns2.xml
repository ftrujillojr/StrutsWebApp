<!--
https://struts.apache.org/docs/result-types.html

chain, freemarker, dispatcher, httpheader, redirect, redirectAction, stream, xslt, tiles, json    

<action name="home" method="noAction" class="org.yourorg.yourapp.controllers.NoAction">
-->
 
<struts>
    <include file="my_common_default.xml"></include>
    
    <package name="sample-pkg" namespace="/ns2" extends="my-common-default">
        
        <action name="dude" method="noAction" class="org.yourorg.yourapp.controllers.NoAction">
            <result name="success" type="tiles">/ns2/dude</result>
        </action>
        
        <action name="home" method="noAction" class="org.yourorg.yourapp.controllers.NoAction">
            <result name="success" type="tiles">/ns2/home</result>
        </action>
        
    </package>
</struts>        
